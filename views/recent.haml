#recent_links
  %center
    %table.table
      %thead
        %th Result
        %th Original
      %tbody
      -Recent.limit(10).each do |r|
        %tr{align: "center"}
          %td
            %a{href: "#{r.result}", target: "blank"}
              %img.thumbnail{src: "#{r.result}"}
          %td
            %a{href: "#{r.source}"} #{Util.shorten r.source}
